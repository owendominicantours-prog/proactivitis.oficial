{
  "name": "proactivitis-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NEXT_DISABLE_SOURCE_MAPS=1 next dev",
    "build": "cross-env NEXT_DISABLE_TURBOPACK=1 NEXT_SKIP_ESLINT=1 prisma generate && cross-env NEXT_DISABLE_TURBOPACK=1 NEXT_SKIP_ESLINT=1 next build",
    "vercel-build": "cross-env NEXT_DISABLE_TURBOPACK=1 NEXT_SKIP_ESLINT=1 prisma generate && cross-env NEXT_DISABLE_TURBOPACK=1 NEXT_SKIP_ESLINT=1 next build",
    "start": "next start",
    "lint": "next lint",
    "lint:ci": "next lint",
    "test": "jest --runInBand",
    "test:e2e": "playwright test",
    "health-check": "node scripts/health-check.cjs",
    "prisma": "prisma",
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts",
    "seed:countries": "node scripts/seed-countries.cjs",
    "generate:sitemap": "ts-node --project tsconfig.seed.json scripts/generate-sitemap.ts",
    "seed:destinations": "node scripts/seed-destinations.ts",
    "hotels:enrich": "node scripts/enrich-hotel-directory-from-official.mjs",
    "blogs:tour-packs": "ts-node --project tsconfig.seed.json scripts/generateTourSeoBlogPacks.ts",
    "blogs:seo-batch1": "ts-node --project tsconfig.seed.json scripts/createSeoKeywordBlogsBatch1.ts",
    "blogs:seo-batch2": "ts-node --project tsconfig.seed.json scripts/createSeoKeywordBlogsBatch2.ts",
    "blogs:seo-batch3": "ts-node --project tsconfig.seed.json scripts/createSeoKeywordBlogsBatch3.ts",
    "blogs:translate": "ts-node --project tsconfig.seed.json scripts/translatePublishedBlogs.ts",
    "blogs:sitemap": "ts-node --project tsconfig.seed.json scripts/generate-blog-sitemap.ts",
    "tours:create-santo-domingo-seo-variants": "ts-node --project tsconfig.seed.json scripts/createSantoDomingoSeoOnlyVariants.ts",
    "tours:create-saona-seo-variants": "ts-node --project tsconfig.seed.json scripts/createSaonaPuntaCanaSeoOnlyVariants.ts",
    "tours:create-viator-inspired-seo-variants": "ts-node --project tsconfig.seed.json scripts/createViatorInspiredSeoVariants.ts",
    "tours:sync-seo-competitor-prices": "ts-node --project tsconfig.seed.json scripts/syncSeoOnlyCompetitorPrices.ts",
    "translate:tours": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/translateTours.ts",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "5.16.1",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.6.0",
    "@types/pdfkit": "^0.17.4",
    "@vercel/analytics": "^1.6.1",
    "@vercel/blob": "^1.0.0",
    "@vercel/speed-insights": "^1.3.1",
    "autoprefixer": "^10.4.22",
    "bcrypt": "^6.0.0",
    "clsx": "^1.2.1",
    "date-fns": "^4.1.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "html-to-text": "^9.0.5",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "next": "^15.5.9",
    "next-auth": "4.24.13",
    "nodemailer": "^7.0.11",
    "o": "^2.3.2",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.2",
    "postcss": "^8.4.33",
    "prisma": "5.16.1",
    "qrcode": "^1.5.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "^0.34.5",
    "sqlite3": "^5.1.4",
    "stripe": "^20.0.0",
    "swr": "^2.3.7",
    "tailwindcss": "^3.4.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.0.0",
    "@playwright/test": "^1.57.0",
    "@types/bcrypt": "^6.0.0",
    "@types/html-to-text": "^9.0.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "20.5.2",
    "@types/nodemailer": "^7.0.4",
    "@types/papaparse": "^5.5.2",
    "@types/qrcode": "^1.5.6",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.9.0",
    "eslint-config-next": "15.5.9",
    "eslint-plugin-react-hooks": "7.0.0",
    "jest": "^30.2.0",
    "prisma": "5.16.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.1",
    "typescript": "5.9.3"
  }
}
